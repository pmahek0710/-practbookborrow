DECLARE
r number;
a number;
BEGIN
FOR r in 5..9 LOOP
a:=3.14*r*r;
INSERT INTO circle(radius,area) VALUES (r,a);
DBMS_OUTPUT.PUT_LINE('radius=' || r || ', area='||a);
END LOOP;
END;
/
SQL> create table circle( radius number, area number);

Table created.

SQL> set serveroutput on

SQL> @C:\Users\Mahek\Desktop\PLsql\prog4.sql;
radius=5, area=78.5
radius=6, area=113.04
radius=7, area=153.86
radius=8, area=200.96
radius=9, area=254.34

